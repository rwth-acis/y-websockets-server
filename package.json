  "license": "MIT",
  "bugs": {
    "url": "https://github.com/y-js/y-websockets-server/issues"
  },
  "homepage": "http://y-js.org",
  "dependencies": {
    "minimist": "^1.2.0",
    "node-fetch": "^2.6.1",
    "socket.io": "1.3.7",
    "y-memory": "^8.0.0",
    "yjs": "^12.1.6"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.4",
    "fs": "0.0.2",
    "gulp": "^3.9.0",
    "gulp-load-plugins": "^1.1.0",
    "gulp-prompt": "^0.1.2",
    "http": "0.0.0",
    "pre-commit": "^1.1.2",
    "run-sequence": "^1.1.4",
    "standard": "^5.3.1"
  }
}
